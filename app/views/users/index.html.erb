<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<%= link_to "New user", new_user_path %>

<div id="users">
  <table class="table">
    <tr>
      <th>Id</th>
      <th>Username</th>
      <th>Dob</th>
      <th>Email</th>
      <th>Action</th>
    </tr>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.id %>
      </td>

      <td>
        <%= user.username %>
      </td>

      <td>
        <%= user.dob %>
      </td>

      <td>
        <%= user.email %>
      </td>

      <td>
        <%= link_to "Show this user", user %>
      </td>
    </tr>
  <% end %>
</div>


