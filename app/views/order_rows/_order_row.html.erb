<div id="<%= dom_id order_row %>">
  <p>
    <strong>Order:</strong>
    <%= order_row.order_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= order_row.order.user.username %>
  </p>

  <p>
    <strong>Product:</strong>
    <%= order_row.product.name %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order_row.quantity %>
  </p>

</div>
